{
    "name": "Problem button on sdamgia.ru",
    "description": "Adds a problem button where there isn't one.",
    "version": "1.3",
    "manifest_version": 3,
    "icons": {
        "16":"icons/16.png",
        "32":"icons/32.png",
        "48":"icons/48.png",
        "128":"icons/128.png"
    },
    "host_permissions": [
        "https://*.sdamgia.ru/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://*.sdamgia.ru/test*"
        ],
        "js": ["script.js"]
      }
    ]
}